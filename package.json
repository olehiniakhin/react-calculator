{
  "name": "react-calculator",
  "private": true,
  "description": "A simple calculator built with react",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{tsx,js,jsx,json,css,md}\"",
    "format-root": "prettier --write \"./*.{tsx,js,jsx,json,css,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "@types/rebass": "^3.0.4",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.1.30",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.14",
    "global": "^4.3.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "rebass": "^3.0.1",
    "styled-components": "^4.1.3",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "babel-jest": "^24.5.0",
    "babel-preset-gatsby": "^0.1.9",
    "esm": "^3.2.20",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "lint-staged": {
    "src/**/*.{tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
